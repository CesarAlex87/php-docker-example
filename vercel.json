{
    "version": 2,
    "builds": [
      {
        "src": "/index.php",
        "use": "vercel-php@0.7.3"
      }
    ],
    "routes": [
      {
        "src": "/assets/css/(.*)",
        "dest": "/assets/css/(.*)"
      },
      {
        "src": "/assets/img/(.*)",
        "dest": "/assets/img/(.*)"
      },
      {
        "src": "/assets/js/(.*)",
        "dest": "/assets/js/(.*)"
      },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }   